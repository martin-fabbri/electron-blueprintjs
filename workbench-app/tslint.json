{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "tailwind.js"
    ]
  },
  "rules": {
    "member-access": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "variable-name": false,
    "max-classes-per-file": false,
    "no-console": false
  }
}
